<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tal.xes.mapper.CountryModelMapper">
  <resultMap id="BaseResultMap" type="com.tal.xes.entity.CountryModel">
    <result column="id" jdbcType="BIGINT" property="id" />
    <result column="country_name" jdbcType="VARCHAR" property="countryName" />
    <result column="region_id" jdbcType="BIGINT" property="regionId" />
  </resultMap>
  <insert id="insert" parameterType="com.tal.xes.entity.CountryModel">
    insert into country (id, country_name, region_id
      )
    values (#{id,jdbcType=BIGINT}, #{countryName,jdbcType=VARCHAR}, #{regionId,jdbcType=BIGINT}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.tal.xes.entity.CountryModel">
    insert into country
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="countryName != null">
        country_name,
      </if>
      <if test="regionId != null">
        region_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=BIGINT},
      </if>
      <if test="countryName != null">
        #{countryName,jdbcType=VARCHAR},
      </if>
      <if test="regionId != null">
        #{regionId,jdbcType=BIGINT},
      </if>
    </trim>
  </insert>
</mapper>